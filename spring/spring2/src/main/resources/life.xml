<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:c="http://www.springframework.org/schema/c"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean name="dto" class="com.chunjae.dto.MemberDTO" c:name="hong" c:age="10"/>
    <!-- init-method, destroy-method 는 가장 위에 설정한 bean이 우선순위가 된다.
         InitializingBean 과 DisposableBean 상속을 한 경우,
         init은 afterPropertiesSet 메서드가 우선이고, destroy는 destroy-method 설정이 가장 마지막에 적용된다. -->
    <bean name="life2" class="com.chunjae.life.LifeCycleDoing2" init-method="a1" destroy-method="b1"/>
    <bean name="life" class="com.chunjae.life.LifeCycleDoing" init-method="a" destroy-method="b"/>


</beans>